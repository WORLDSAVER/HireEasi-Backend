{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strict": false,
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "types": [
         "node"
      ],
      "resolveJsonModule": true,
      "allowJs": true,
      "downlevelIteration": true
   },
   "include": [
      "src/*",
      "src/entity/*",
      "src/migration/*"
   ],
   "exclude": [
      "node_modules",
      "./node_modules",
      "./node_modules/*",
      "./node_modules/@types/node/index.d.ts"
   ]
}
